      "\t\t\\begin{column}{0.$3\\textwidth}",
      "\t\t\t$2",
      "\t\t\\end{column}",
      "\t\t\\begin{column}{0.$1\\textwidth}",
      "\t\t\t$0",
      "\t\t\\end{column}",
      "\t\\end{columns}"
    ],
    "description": "Beamer columns"
  },
  "Column": {
    "prefix": "column",
    "body": ["\t\t\\begin{column}{0.$1\\textwidth}", "\t\t\t$0", "\t\t\\end{column}"],
    "description": "Beamer column"
  }
}
